{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug CRA (Edge)",
      "type": "msedge",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*",
        "webpack://*/./*": "${webRoot}/*",
        "webpack:///src/*": "${webRoot}/src/*",
        "webpack://*/src/*": "${webRoot}/src/*"
      }
    }
  ]
}
